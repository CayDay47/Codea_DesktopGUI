
--# Main
-- PC Touch

-- Main

supportedOrientations(LANDSCAPE_ANY)
displayMode(FULLSCREEN_NO_BUTTONS)
rectMode(RADIUS)
spriteMode(RADIUS)
ellipseMode(RADIUS)
textAlign(CENTER)

function setup()
    
    -- PC
    desktop_status = "Active"
    
    parameter.watch("desktop_status")
    parameter.watch("AirHockey_status")
    
    -- Images
    button = Images:getButton()
    pauseButton = Images:getPauseButton()
    AirHockeyLogo = Images:getAirHockeyLogo()
    t = Images:getWindowTop()
    b = Images:getWindowBottom()
    l = Images:getWindowLeft()
    r = Images:getWindowRight()
    tlc = Images:getWindowTlc()
    trc = Images:getWindowTrc()
    blc = Images:getWindowBlc()
    brc = Images:getWindowBrc()
    d = Images:getWindowDrag()
    
    -- Button
    buttonShutDown = Button("Shut Down",WIDTH - 50,HEIGHT - 15,50,15,0,0,15,255,0,0,button,close)
    buttonKeyboardOpen = Button("Keyboard",WIDTH - 50,HEIGHT - 75,50,15,0,0,15,90,175,250,button,showKeyboard)
    buttonAlert = Button("Alert",WIDTH - 50,HEIGHT - 125,50,15,0,0,15,90,175,250,button,alert,"You pressed the alert button. Alert! Alert! Alert!")
    buttonAirHockey = Button("Air Hockey",100,HEIGHT - 100,50,50,0,-60,20,255,255,255,AirHockeyLogo,AirHockey_activate)
    
    allButton = AllButton({buttonShutDown,buttonKeyboardOpen,buttonAlert,buttonAirHockey})
    
    -- Textbox
    textbox1 = Textbox("Testing",WIDTH/2,HEIGHT-HEIGHT/10,100,50,30,255,50,100,150)
    
    -- Window
    wWindow1 = Window("Window1",WIDTH/2,HEIGHT/2,50,50,t,b,l,r,tlc,trc,blc,brc,d)
    wWindow2 = Window("Window2",WIDTH/2 + 100,HEIGHT/2 + 100,50,50,t,b,l,r,tlc,trc,blc,brc,d)
    wWindow3 = Window("Window3",WIDTH/2 - 100,HEIGHT/2 -100,50,50,t,b,l,r,tlc,trc,blc,brc,d)
    wWindow4 = Window("Window4",WIDTH/1.5,HEIGHT/2 -100,50,50,t,b,l,r,tlc,trc,blc,brc,d)
    
    parameter.color("Tint Colour",255,255,255,255)
    
    allWindow = AllWindow({wWindow1,wWindow2,wWindow3,wWindow4})
    
    -- FPS
    fps = FPS()
    
end

function touched(t)
    
    --Desktop
    if desktop_status == "Active" then
    -- Button
    allButton:touched(t)
    
    -- Window
    allWindow:touched(t)
    end
        -- Neon Air Hockey
    if AirHockey_status == "Active" then
        AirHockey_touched(t)
    end
    
end

function draw()
    
    -- Desktop
    desktop = "Active"
    
    background(255, 255, 255, 255)
    sprite("Documents:BackGround2",WIDTH/2,HEIGHT/2,WIDTH/2,HEIGHT/2)
    
    --Button
    if desktop_status == "Active" then
        allButton:draw()
        
        -- Textbox
        textbox1:draw()
        
        -- Window
        
        pushMatrix()
        allWindow:draw()
        popMatrix()
        
        -- FPS
        fps:draw()
        
        pushStyle()
        fill(255, 0, 0, 255)
        ellipse(CurrentTouch.x,CurrentTouch.y,5)
        popStyle()
        
        
    end
    
    -- Neon Air Hockey
    if AirHockey_status == "Active" then
        desktop_status = "Inactive"
        AirHockey_active()
        else AirHockey_status = "Inactive"
        desktop_status = "Active"
    end
    
end

-- 1024 * 768


--# Button
-- Button

Button = class()

function Button:init(name,x,y,width,height,textx,texty,fontsize,colourR,colourG,colourB,buttonImage,function0,function01,function02,function03,function04)
    
    self.name = name
    self.x = x
    self.y = y
    self.width = width
    self.height = height
    self.textx = textx
    self.texty = texty
    self.fontsize = fontsize
    self.colourR = colourR
    self.colourG = colourG
    self.colourB = colourB
    self.buttonImage = buttonImage
    self.function0 = function0 or nil
    self.function01 = function01 or nil
    self.function02 = function02 or nil
    self.function03 = function03 or nil
    self.function04 = function04 or nil
    
    self.pressed = "No"
    self.released = "No"
    self.tint = "No"
    
end

function Button:touched(t)
    if pressedField(self.x,self.y,self.width,self.height,t) == true then
        self.pressed = "Yes"
    end
    
    if self.pressed == "Yes" and t.x<=self.x+self.width and t.x>=self.x-self.width and t.y<=self.y+self.height and t.y>=self.y-self.height then
        self.tint = "Yes"
        else self.tint = "No"
    end
    
    if releasedField(self.x,self.y,self.width,self.height,t) == true and self.pressed == "Yes" then
        self.released = "Yes"
        self.pressed = "No"
    end
    
    if self.released == "Yes"
    then if self.function0 ~= nil then
            if self.function01 == nil
            then self.function0()
                else
                if self.function04 ~= nil
                then self.function0(self.function01,self.function02,self.function03,self.function04)
                end
                if self.function03 ~= nil
                then self.function0(self.function01,self.function02,self.function03)
                end
                if self.function02 ~= nil
                then self.function0(self.function01,self.function02)
                end
                if self.function01 ~= nil
                then self.function0(self.function01)
                end
            end
        end
    end
    
    self.released = "No"
    
    if t.state == ENDED then
        self.pressed = "No"
        self.tint = "No"
    end
    
end

function Button:draw()
    
    pushStyle()
    if self.tint == "Yes" then
        tint(self.colourR*1.5,self.colourG*1.5,self.colourB*1.5,225)
        elseif self.tint == "No" then
        tint(self.colourR,self.colourG,self.colourB,225)
    end
    sprite(self.buttonImage,self.x,self.y,self.width,self.height)
    fill(255, 255, 255, 255)
    fontSize(self.fontsize)
    textWrapWidth(self.width*2-self.width/20)
    text(self.name,self.x+self.textx,self.y+self.texty)
    popStyle()
    
end

--# ClassAll
-- ClassAll

-- ButtonAll

AllButton = class(Button)
 
function AllButton:init(button)
   self.button = button
end
 
function AllButton:touched(t)
   for _, c in pairs(self.button) do
       c:touched(t)
   end
end
 
function AllButton:draw()
   for _, c in pairs(self.button) do
       c:draw()
   end
end

 
-- WindowAll

AllWindow = class(Window)
 
function AllWindow:init(window)
   self.window = window
end
 
function AllWindow:touched(t)
   for _, c in pairs(self.window) do
       c:touched(t)
   end
end
 
function AllWindow:draw()
   for _, c in pairs(self.window) do
       c:draw()
   end
end
 

--# FPS
FPS = class()
 
function FPS:init()
   self.val = 0
end
 
function FPS:draw()
    pushStyle()
   -- update FPS value with some smoothing
   self.val = self.val*0.99+ 1/(DeltaTime)*0.01
   -- write the FPS on the screen
   fill(208, 208, 208, 255)
   fontSize(30)
    font("AmericanTypewriter")
   rectMode(CENTER)
   text(math.floor(self.val).." fps",50,HEIGHT-25)
    popStyle()
end

--# Functions
-- Functions

function touchField(x,y,width,height)
    if CurrentTouch.x <= x + width and CurrentTouch.x >= x - width and CurrentTouch.y <= y + height and CurrentTouch.y >= y - height and CurrentTouch.state == BEGAN
    then return true
    else return false end
end

function pressedField(x,y,width,height,t)
    if t.x <= x + width and t.x >= x - width and t.y <= y + height and t.y >= y - height and t.state == BEGAN
    then return true
    else return false end
end

function releasedField(x,y,width,height,t)
    if t.x <= x + width and t.x >= x - width and t.y <= y + height and t.y >= y - height and t.state == ENDED
    then return true
    else return false end
end
