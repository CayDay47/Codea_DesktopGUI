--# AirHockey
-- Air Hockey

function AirHockey_activate()
    
    AirHockey_status = "Active"
    
    -- Game
    menu = true
    gameSetup = false
    roundNew = false
    gamePaused = false
    gamePlay = false
    gamePlayTouch = false
    resultsGame = false
    winScore = 7
    
    -- Buttons
    AirHockey_buttonStart = Button("Start",WIDTH/2,HEIGHT/3*1,WIDTH/5.12,HEIGHT/7.68,0,0,HEIGHT/6,0,255,0,button,AirHockey_startNewGame)
    AirHockey_buttonMenu = Button("Return to Menu",WIDTH/2,HEIGHT/5,WIDTH/4.096,HEIGHT/19.2,0,0,HEIGHT/15.26,0,255,0,button,AirHockey_startMenu)
    AirHockey_buttonX = Button("X",WIDTH-WIDTH/15,HEIGHT-HEIGHT/30,WIDTH/15,HEIGHT/30,0,0,HEIGHT/15.26,255,0,0,button,AirHockey_deactivate)
    AirHockey_buttonPause1 = Button("",WIDTH/10.24,HEIGHT/7.68,WIDTH/25.6,HEIGHT/19.2,0,0,0,150,150,150,pauseButton)
    AirHockey_buttonPause2 = Button("",WIDTH/10.24,HEIGHT-HEIGHT/7.68,WIDTH/25.6,HEIGHT/19.2,0,0,0,150,150,150,pauseButton)
    AirHockey_buttonPause3 = Button("",WIDTH-WIDTH/10.24,HEIGHT/7.68,WIDTH/25.6,HEIGHT/19.2,0,0,0,150,150,150,pauseButton)
    AirHockey_buttonPause4 = Button("",WIDTH-WIDTH/10.24,HEIGHT-HEIGHT/7.68,WIDTH/25.6,HEIGHT/19.2,0,0,0,150,150,150,pauseButton)
    AirHockey_buttonResume = Button("Resume Game",WIDTH/2,HEIGHT/5*2,WIDTH/3.2,HEIGHT/7.68,0,0,HEIGHT/10,0,255,0,button,AirHockey_resumeGame)
    
    -- Physics
    physics.continuous = true
end

function AirHockey_active()
    background(0, 0, 0, 255)
    if menu == true then
        AirHockey_drawMenu()
    end
    if gameSetup == true then
        AirHockey_setupGame()
        gamePlay = true
        gameSetup = false
    end
    if roundNew == true then
        AirHockey_newRound()
        gamePlay = true
        roundNew = false
    end
    if gamePlay == true then
        gamePlayTouch = true
        AirHockey_playGame()
        else gamePlayTouch = false
    end
    if gamePaused == true then
        gamePlayTouch = false
        AirHockey_pauseGame()
    end
    if resultsGame == true then
        gamePlayTouch = false
        AirHockey_gameResults()
    end
end

function AirHockey_touched(t)
    if menu == true then
        AirHockey_buttonStart:touched(t)
        AirHockey_buttonX:touched(t)
    end
    if gamePaused == true then
        AirHockey_buttonResume:touched(t)
        AirHockey_buttonMenu:touched(t)
        AirHockey_buttonX:touched(t)
    end
    if resultsGame == true then
        AirHockey_buttonMenu: touched(t)
        AirHockey_buttonX:touched(t)
    end
    
    if gamePlay == true then
        AirHockey_buttonPause1:touched(t)
        AirHockey_buttonPause2:touched(t)
        AirHockey_buttonPause3:touched(t)
        AirHockey_buttonPause4:touched(t)
        
        if gamePlayTouch == true then
            if t.state==BEGAN then
                if t.x>WIDTH/2 then
                    Scarlet.position=vec2(t.x,t.y)
                    id1=t.id
                end
                if t.x<WIDTH/2 then
                    Cyan.position=vec2(t.x,t.y)
                    id2=t.id
                end
            end
            if t.state==MOVING then
                if t.id==id1 then
                    Scarlet.position=vec2(t.x,t.y)
                    Scarlet.linearVelocity=vec2(t.deltaX*50,t.deltaY*50)
                end
                if t.id==id2 then
                    Cyan.position=vec2(t.x,t.y)
                    Cyan.linearVelocity=vec2(t.deltaX*50,t.deltaY*50)
                end
            end
            if t.state==ENDED then
                if t.id==id1 then
                    id1=0
                    Scarlet.linearVelocity=vec2(0,0)
                end
                if t.id==id2 then
                    id2=0
                    Cyan.linearVelocity=vec2(0,0)
                end
            end
        end
    end
end

function AirHockey_drawMenu()
    --backingMode(STANDARD)
    pushStyle()
    fill(0, 0, 0, 255)
    rect(WIDTH/2,HEIGHT/2,WIDTH/2,HEIGHT/2)
    font("Futura-MediumItalic")
    fontSize(200)
    fill(0, 255, 0, 255)
    text("Air",WIDTH/2,HEIGHT/5*4)
    font("Optima-Regular")
    fontSize(75)
    fill(0, 255, 0, 255)
    text("Hockey",WIDTH/2,HEIGHT/5*3)
    
    AirHockey_buttonStart:draw()
    font("Copperplate-Bold")
    AirHockey_buttonX:draw()
    
    popStyle()
end

function AirHockey_startNewGame()
    menu = false
    gameSetup = true
end

function AirHockey_setupGame()
    -- Game
    scoreScarlet = 0
    scoreCyan = 0
    
    -- variables for various functions and positions
    ballpos = vec2(WIDTH/2,HEIGHT/2)
    ballLinearVelocity = nil
    
    -- Border
    border1 = physics.body(CHAIN,loop,vec2(0,0),vec2(0,HEIGHT/2-HEIGHT/7.68),vec2(WIDTH/51.2,HEIGHT/2-HEIGHT/7.68),vec2(WIDTH/51.2,HEIGHT/38.4),vec2(WIDTH-WIDTH/51.2,HEIGHT/38.4),vec2(WIDTH-WIDTH/51.2,HEIGHT/2-HEIGHT/7.68),vec2(WIDTH,HEIGHT/2-HEIGHT/7.68),vec2(WIDTH,0))
    border1.type = STATIC
    border1.bullet = true
    border2 = physics.body(CHAIN,loop,vec2(0,HEIGHT),vec2(0,HEIGHT/2+HEIGHT/7.68),vec2(WIDTH/51.2,HEIGHT/2+HEIGHT/7.68),vec2(WIDTH/51.2,HEIGHT-HEIGHT/38.4),vec2(WIDTH-WIDTH/51.2,HEIGHT-HEIGHT/38.4),vec2(WIDTH-WIDTH/51.2,HEIGHT/2+HEIGHT/7.68),vec2(WIDTH,HEIGHT/2+HEIGHT/7.68),vec2(WIDTH,HEIGHT))
    border2.type = STATIC
    border2.bullet = true
    
    -- Ball
    ball = physics.body(CIRCLE,WIDTH/40.96)
    ball.type = DYNAMIC
    ball.position = ballpos
    ball.gravityScale = 0
    ball.sleepingAllowed = false
    ball.restitution=.8
    ball.bullet=true
    ball.linearVelocity=vec2(0,0)
    
    -- Player 1's Paddle
    Scarlet = physics.body(CIRCLE,WIDTH/20.48)
    Scarlet.type = KINEMATIC
    Scarlet.position = vec2(WIDTH/4*3, HEIGHT/2)
    Scarlet.gravityScale = 0
    Scarlet.sleepingAllowed = false
    
    -- Player 2's Paddle
    Cyan = physics.body(CIRCLE,WIDTH/20.48)
    Cyan.type = KINEMATIC
    Cyan.position = vec2(WIDTH/4, HEIGHT/2)
    Cyan.gravityScale = 0
    Cyan.sleepingAllowed = false
end

function AirHockey_newRound()
    -- variables for various functions and positions
    ballpos = vec2(WIDTH/2,HEIGHT/2)
    
    -- Players
    Scarlet.position = vec2(WIDTH/4*3, HEIGHT/2)
    Scarlet.linearVelocity = vec2(0,0)
    Cyan.position = vec2(WIDTH/4, HEIGHT/2)
    Cyan.linearVelocity = vec2(0,0)
    
    -- Ball
    ball.position = ballpos
    ball.linearVelocity=vec2(0,0)
end

function AirHockey_playGame()
    pushStyle()
    
    -- Frame
    strokeWidth(10)
    stroke(31, 100, 31, 255)
    lineCapMode(SQUARE)
    line(WIDTH/2,HEIGHT/38.4,WIDTH/2,HEIGHT-HEIGHT/38.4)
    
    strokeWidth(20)
    stroke(0, 255, 0, 255)
    fill(0, 0, 0, 0)
    
    line(WIDTH/102.4,HEIGHT/2-HEIGHT/7.68,WIDTH/102.4,0)
    line(0,HEIGHT/76.8,WIDTH,HEIGHT/76.8)
    line(WIDTH-WIDTH/102.4,0,WIDTH-WIDTH/102.4,HEIGHT/2-HEIGHT/7.68)
    
    line(WIDTH/102.4,HEIGHT/2+HEIGHT/7.68,WIDTH/102.4,HEIGHT)
    line(0,HEIGHT-HEIGHT/76.8,WIDTH,HEIGHT-HEIGHT/76.8)
    line(WIDTH-WIDTH/102.4,HEIGHT,WIDTH-WIDTH/102.4,HEIGHT/2+HEIGHT/7.68)
    
    -- Pause System
    if AirHockey_buttonPause1.pressed == "Yes" and AirHockey_buttonPause2.pressed == "Yes" then
        AirHockey_buttonPause1.pressed = "No"
        AirHockey_buttonPause2.pressed = "No"
        ballLinearVelocity = ball.linearVelocity
        gamePaused = true
    end
    if AirHockey_buttonPause3.pressed == "Yes" and AirHockey_buttonPause4.pressed == "Yes" then
        AirHockey_buttonPause3.pressed = "No"
        AirHockey_buttonPause4.pressed = "No"
        ballLinearVelocity = ball.linearVelocity
        gamePaused = true
    end
    
    -- Ball
    ballpos = ball.position
    strokeWidth(0)
    fill(255, 255, 255, 255)
    ellipse(ball.position.x,ball.position.y,WIDTH/40.96)
    
    -- Scarlet Player
    Scarlet.x = math.max(WIDTH/2+50,Scarlet.x)
    Scarlet.x = math.min(WIDTH-70,Scarlet.x)
    Scarlet.y = math.max(70,Scarlet.y)
    Scarlet.y = math.min(HEIGHT-70,Scarlet.y)
    strokeWidth(30)
    stroke(255,36,0,255)
    fill(50, 8, 0, 255)
    ellipse(Scarlet.position.x,Scarlet.position.y, WIDTH/20.48)
    
    -- Player 2
    Cyan.x = math.max(70,Cyan.x)
    Cyan.x = math.min(WIDTH/2-50,Cyan.x)
    Cyan.y = math.max(70,Cyan.y)
    Cyan.y = math.min(HEIGHT-70,Cyan.y)
    strokeWidth(30)
    stroke(0,255,255,255)
    fill(0, 50, 50, 255)
    ellipse(Cyan.position.x, Cyan.position.y, WIDTH/20.48)
    
    -- Buttons
    AirHockey_buttonPause1:draw()
    AirHockey_buttonPause2:draw()
    AirHockey_buttonPause3:draw()
    AirHockey_buttonPause4:draw()
    
    -- Scoring System
    if ballpos.x <= 0 then
        scoreScarlet = scoreScarlet + 1
        roundNew = true
        gamePlay = false
    end
    if ballpos.x >= WIDTH then
        scoreCyan = scoreCyan + 1
        roundNew = true
        gamePlay = false
    end
    
    font("Copperplate-Bold")
    fontSize(75)
    fill(255, 36, 0, 255)
    text(scoreScarlet,WIDTH/2,HEIGHT-HEIGHT/15)
    fill(0, 255, 255, 255)
    text(scoreCyan,WIDTH/2,HEIGHT/15)
    popMatrix()
    
    if scoreScarlet == winScore or scoreCyan == winScore then
        resultsGame = true
    end
    
    popStyle()
end

function AirHockey_pauseGame()
    pushStyle()
    
    ball.linearVelocity = vec2(0,0)
    
    gamePlayTouch = false
    
    fill(63.5, 63.5, 63.5, 200)
    rect(WIDTH/2,HEIGHT/2,WIDTH*9/10/2,HEIGHT*9/10/2)
    font("Copperplate-Light")
    fill(0, 255, 0, 255)
    fontSize(75)
    text("Paused",WIDTH/2,HEIGHT-HEIGHT*1/5)
    font("Copperplate")
    fontSize(100)
    
    -- Current Score
    font("Copperplate-Bold")
    fontSize(159)
    textAlign(CENTER)
    fill(0, 255, 0, 255)
    text(":",WIDTH/2,HEIGHT/3*2)
    fill(225,36,0,255)
    text(scoreScarlet,WIDTH*4/10,HEIGHT/3*2)
    fill(0,255,255,255)
    text(scoreCyan,WIDTH*6/10,HEIGHT/3*2)
    
    -- Buttons
    font("Optima-Regular")
    AirHockey_buttonResume:draw()
    AirHockey_buttonMenu:draw()
    font("Copperplate-Bold")
    AirHockey_buttonX:draw()
    
    popStyle()
end

function AirHockey_resumeGame()
    gamePaused = false
    gamePlayTouch = true
    ball.linearVelocity = ballLinearVelocity
end

function AirHockey_gameResults()
    pushStyle()
    fill(63.5, 63.5, 63.5, 200)
    rect(WIDTH/2,HEIGHT/2,WIDTH*9/10/2,HEIGHT*9/10/2)
    font("Copperplate-Light")
    fill(0, 255, 0, 255)
    fontSize(75)
    text("Results",WIDTH/2,HEIGHT-HEIGHT*1/5)
    font("Copperplate")
    fontSize(100)
    if scoreScarlet == winScore then
        fill(225,36,0,255)
        text("Scarlet Wins",WIDTH/2,HEIGHT*3/5)
    end
    if scoreCyan == winScore then
        fill(0,255,255,255)
        text("Cyan Wins",WIDTH/2,HEIGHT*3/5)
    end
    
    font("Copperplate-Bold")
    fontSize(100)
    textAlign(CENTER)
    fill(0, 255, 0, 255)
    text(":",WIDTH/2,HEIGHT*2/5)
    fill(225,36,0,255)
    text(scoreScarlet,WIDTH*4/10,HEIGHT*2/5)
    fill(0,255,255,255)
    text(scoreCyan,WIDTH*6/10,HEIGHT*2/5)
    
    -- Buttond
    AirHockey_buttonMenu:draw()
    AirHockey_buttonX:draw()
    popStyle()
end

function AirHockey_startMenu()
    resultsGame = false
    gamePaused = false
    gamePlay = false
    
    border1:destroy()
    border1 = nil
    border2:destroy()
    border2 = nil
    ball:destroy()
    ball = nil
    Scarlet:destroy()
    Scarlet = nil
    Cyan:destroy()
    Cyan = nil
    
    menu = true
end

function AirHockey_deactivate()
    
    AirHockey_status = "Inactive"
    
    menu = nil
    gameSetup = nil
    roundNew = nil
    gamePaused = nil
    gamePlay = nil
    gamePlayTouch = nil
    resultsGame = nil
    winScore = nil
    scoreScarlet = nil
    scoreCyan = nil
    
    -- Deactivate Buttons
    AirHockey_buttonStart = nil
    AirHockey_buttonMenu = nil
    AirHockey_buttonX = nil
    AirHockey_buttonPause1 = nil
    AirHockey_buttonPause2 = nil
    AirHockey_buttonPause3 = nil
    AirHockey_buttonPause4 = nil
    AirHockey_buttonResume = nil
    
    -- variables for various functions and positions
    ballpos = nil
    ballLinearVelocity = nil
    
    -- Physics
    if ball ~= nil then
        ball:destroy()
        ball = nil
    end
    if Scarlet ~= nil then
        Scarlet:destroy()
        Scarlet = nil
    end
    if Cyan ~= nil then
        Cyan:destroy()
        Cyan = nil
    end
    if border1 ~= nil then
        border1:destroy()
        border1 = nil
    end
    if border2 ~= nil then
        border2:destroy()
        border2 = nil
    end
    
end

--# SpritelyLoader
SpritelyLoader = class()

-- SpriteLoader 
-- ver. 1.0
-- moves data from global to project storage
-- ====================

-- This class takes data from global and dumps it into your project.
-- 
-- To use this class, create an instance in your project.
-- For example:  aloader = SpritelyLoader()
--
-- This call will read both keys and images from the global Spritely
-- storage and copy them into your project space. 
--
-- Once in the project data, load the saved string to 
-- an image using it's key.  For example:
--
-- createImage = loadstring( readProjectData("bob"))
-- anImage = createImage() 

-- To display your images, you'll need to load them into
-- a sprite. The command looks like this

-- sprite(anImage, 100, 100, 32, 32)

-- Good luck!

function SpritelyLoader:init()
    local keys, k, s
    if readGlobalData("SpritelyKeys") == nil then
        print("No Spritely data found.")
    else
        -- read global keys
        keys = readGlobalData("SpritelyKeys")
        saveProjectData("SpritelyKeys", keys)
        print("Moving...")
        for k in string.gmatch(keys,"([^,]+)") do
            print(k)
            s = readGlobalData(k)
            saveProjectData(k, s)
        end
        print("Move complete.")
    end
    
end



--# Textbox
Textbox = class()

function Textbox:init(text,x,y,width,height,fontsize,colourR,colourG,colourB,colourA)
    self.text = text
    self.x = x
    self.y = y
    self.width = width
    self.height = height
    self.fontsize = fontsize
    self.colourR = colourR
    self.colourG = colourG
    self.colourB = colourB
    self.colourA = colourA
    
    self.pressed = "No"
    self.released = "No"
    self.selected = false
end

function Textbox:touched(touch)
        if pressedField(self.x,self.y,self.width,self.height,t) == true then
        self.pressed = "Yes"
    end
    
    if releasedField(self.x,self.y,self.width,self.height,t) == true and self.pressed == "Yes" then
        self.released = "Yes"
        self.pressed = "No"
    end
    
    if self.released == "Yes"
    then self.selected = true
    end
    
    self.released = "No"
    
    if t.state == ENDED then
        self.pressed = "No"
    end
    
end

function Textbox:draw()
    pushStyle()
    fill(self.colourR,self.colourG,self.colourB,self.colourA)
    rect(self.x,self.y,self.width,self.height)
    
    fontSize(self.fontsize)
    text(self.text,self.x,self.y)
    popStyle()
end

--# Window
-- Window

Window = class()

function Window:init(name,x,y,width,height,top,bottom,left,right,topLeftCorner,topRightCorner,bottomLeftCorner,bottomRightCorner,drag)
    
    self.name = name
    self.minx = x
    self.miny = y
    self.x = x
    self.y = y
    self.width = width
    self.height = height
    self.top = top
    self.bottom = bottom
    self.left = left
    self.right = right
    self.topLeftCorner = topLeftCorner
    self.topRightCorner = topRightCorner
    self.bottomLeftCorner = bottomLeftCorner
    self.bottomRightCorner = bottomRightCorner
    self.drag = drag
    
    self.moving = false
    self.selected = "None"
end

function Window:touched(t)
    
    -- print(self.selected)
    if t.state == ENDED
    then self.selected = "None"
    end
    if t.state == BEGAN
    then
        if touchField(self.x,self.y + self.height,self.width - 5,5,t) == true
        then self.selected = "Top"
        end
        if touchField(self.x,self.y - self.height,self.width - 5,5,t) == true
        then self.selected = "Bot"
        end
        if touchField(self.x + self.width,self.y,5,self.height - 5,t) == true
        then self.selected = "Left"
        end
        if touchField(self.x - self.width,self.y,5,self.height - 5,t) == true
        then self.selected = "Right"
        end
        if touchField(self.x - self.width,self.y - self.height,5,5,t) == true
        then self.selected = "Blc"
        end
        if touchField(self.x - self.width,self.y + self.height,5,5,t) == true
        then self.selected = "Tlc"
        end
        if touchField(self.x + self.width,self.y + self.height,5,5,t) == true
        then self.selected = "Trc"
        end
        if touchField(self.x + self.width,self.y - self.height,5,5,t) == true
        then self.selected = "Brc"
        end
        if touchField( self.x, self.y + self.height - 15,self.width - 5,10,t) == true
        then self.selected = "Drag"
        end
        if touchField(self.x,self.y - 10,self.width - 5, self.height - 15,t) == true
        then self.selected = "Contents"
        end
    end
    
    self.moving = true
    if self.moving == true and self.selected == "Top"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height + t.deltaY/2,20)
    end
    if self.moving == true and self.selected == "Bot"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height - t.deltaY/2,20)
    end
    if  self.moving == true and self.selected == "Left"
    then self.x = self.x + t.deltaX/2
        self.width = math.max(self.width + t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Right"
    then self.x = self.x + t.deltaX/2
        self.width = math.max(self.width - t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Tlc"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height + t.deltaY/2,20)
        self.x = self.x + t.deltaX/2
        self.width = math.max(self.width - t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Trc"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height + t.deltaY/2,20)
        self.x = self.x + t.deltaX/2
        self.width = math.max(self.width + t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Blc"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height - t.deltaY/2,20)
        self.x = self.x + t.deltaX/2
        self.width = math.max(self.width - t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Brc"
    then self.y = self.y + t.deltaY/2
        self.height = math.max(self.height - t.deltaY/2,20)
        self.x = self.x + t.deltaX/2
        self.width = math.max(self.width + t.deltaX/2,20)
    end
    if self.moving == true and self.selected == "Drag"
    then self.y = self.y + t.deltaY
        self.x = self.x + t.deltaX
    end
    self.moving = false
end

function Window:draw()
    
    self.x = math.max(0,self.x)
    self.x = math.min(WIDTH,self.x)
    self.y = math.max(0,self.y)
    self.y = math.min(HEIGHT,self.y)
    
    pushStyle()
    
    fill(255, 255, 255, 255)
    rect(self.x,self.y - 10,self.width - 7, self.height - 17)
    
    tint(Tint_Colour)
    noSmooth()
    sprite(self.top,self.x,self.y + self.height,self.width - 7.5,7.5)
    sprite(self.bottom,self.x,self.y - self.height,self.width - 7.5,7.5)
    sprite(self.left,self.x - self.width,self.y,7.5,self.height - 7.5)
    sprite(self.right,self.x + self.width,self.y,7.5,self.height - 7.5)
    
    sprite(self.topLeftCorner,self.x - self.width,self.y + self.height,7.5,7.5)
    sprite(self.topRightCorner,self.x + self.width,self.y + self.height,7.5,7.5)
    sprite(self.bottomRightCorner,self.x + self.width,self.y - self.height,7.5,7.5)
    sprite(self.bottomLeftCorner,self.x - self.width,self.y - self.height,7.5,7.5)
    
    sprite(self.drag, self.x, self.y + self.height - 17.5,self.width - 7.5,10)
    
    font("Helvetica")
    fontSize(17.5)
    fill(255, 255, 255, 255)
    if self.width/4.5 >= string.len(self.name)
    then text(self.name,self.x,self.y + self.height - 17.5)
        elseif self.width/3 >= string.len(self.name)
        then text(string.sub(self.name,0,string.len(self.name)/3).."...",self.x,self.y + self.height - 17.5)
        elseif self.width/2.25 >= string.len(self.name)
        then text(string.sub(self.name,0,string.len(self.name)/9).."...",self.x,self.y + self.height - 17.5)
        else text(string.sub(self.name,0,string.len(self.name)/27).."...",self.x,self.y + self.height - 17.5)
    end
    
    popStyle()
end


